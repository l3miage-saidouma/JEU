        targetNamespace="www.univ-grenoble-alpes.fr/projetJeuVideo"
        xmlns:co="www.univ-grenoble-alpes.fr/projetJeuVideo"
        elementFormDefault="qualified"
>
    <!--element racine-->
    <element name="position" type="co:Position"/>

    <!--complexetype de Coordinate-->
    <complexType name="Position">
        <sequence>
            <element name="X" type="int"/>
            <element name="Y" type="int"/>
        </sequence>
    </complexType>

</schema>